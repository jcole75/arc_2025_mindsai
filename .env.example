# ARC 2025 Solution â€“ Example Environment File

# Execution environment: local, kaggle, or skypilot (auto-detected if unset)
ENVIRONMENT=local

# Data + logs
ARC_DATA_PATH=./data/arc-agi-1-5_challenges.json
ARC_LOG_DIR=logs/arc
RUN_LOGS_DIR=run_logs
ARC_TRAINING_LOG_DIR=training_logs

# Credentials (fill in when running on Kaggle/HF)
KAGGLE_USERNAME=your_kaggle_name
KAGGLE_KEY=your_kaggle_key
HUGGINGFACE_TOKEN=your_hf_token
GEMINI_API_KEY=your_gemini_api_key

# Sample budgets + flags
GLOBAL_TTT_SAMPLES=45000
GLOBAL_INF_SAMPLES=10000
GLOBAL_TTT_ENABLED=true
TEST_MODE=false

# Model config overrides (comma-separated module names without package prefix)
# MODEL_CONFIG_MODULES=codet5_small,codet5_large
# 2025 Solution models
MODEL_CONFIG_MODULES=codet5_660m.py,codet5_660m_scr.py

# Optional: TPU/Flax toggles
USE_TPU=false
USE_FLAX_GPU=false

